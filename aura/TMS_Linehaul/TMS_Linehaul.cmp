    <aura:attribute name="filteredUsers" type="List" />
    <aura:attribute name="finalisedNumber" type= "String"/>
    <aura:attribute name="finalisedNumberRoad" type= "String"/>
    <aura:attribute name="finalisedNumberBD" type="String"/>
    <aura:attribute name="finalisedAirNumber" type="string"/>
    <aura:attribute name="isDraftContext" type="Boolean" default="false"/>
    <aura:attribute name="showContinueButton" type="Boolean" default="false"/>
    <aura:attribute name="Filter" type= "String"/>
