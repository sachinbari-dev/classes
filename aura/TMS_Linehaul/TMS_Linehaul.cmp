<aura:component controller="TMS_LinehaulClass" implements="force:appHostable,flexipage:availableForAllPageTypes,force:hasRecordId,flexipage:availableForRecordHome" access="global" >
    
  <aura:attribute name="OriginCity" type="String"/>
