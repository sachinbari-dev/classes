                                        <label class="slds-form-element__label" for="text-input-id-1">Filter</label>
                                        <div class="slds-form-element__control">
                                            
                                            <lightning:button variant="neutral" label="Filter" title="Brand action" onclick="{!c.FilterBag}" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                
