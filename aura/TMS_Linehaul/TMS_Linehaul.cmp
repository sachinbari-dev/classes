    <aura:attribute name="simplenewOversizeBag" type="string"  />
    <aura:attribute name="simpleAirlineTrunk" type="string"  />
    <aura:attribute name="selectedCount" type="Integer" default="0"/>
    <aura:attribute name="ScanselectedCount" type="Integer" default="0"/>
    <aura:attribute name="SearchselectedCount" type="Integer" default="0"/>
