    public static Decimal LineHaulUpdatesFromJCHelper(List<Linehaul_Tracking__c> lhtRecords)
    {
        System.debug('5 PickupUpdates lhtRecords:'+lhtRecords.size());
        Decimal toatValReturn=0;
        Map<String,Linehaul_Tracking__c> lhMap = new Map<String,Linehaul_Tracking__c>();
        if(lhtRecords.size()>0)
        {
            for(Linehaul_Tracking__c lhRecord:lhtRecords)
            {
                lhMap.put(lhRecord.Id,lhRecord);                 
            }            
        }  
        List<Linehaul_Tracking__c> lhListToUpdate = new List<Linehaul_Tracking__c>();        
        if(lhMap.size()>0)
        {            
            
            if(lhMap.size()>0)
            {
                List<AggregateResult> results = [ select Linehaul_Tracking__c,SUM(Shipment_Value__c) totalValue 
                                                 from Secure_Bag__c where Linehaul_Tracking__c IN : lhMap.keySet() Group By Linehaul_Tracking__c];
                System.debug('32 PickupUpdates results:'+results.size());
                Map<Id, Decimal> pickupValueTotal = new Map<Id, Decimal>();
                if(results.size()>0)
                {
                    for (AggregateResult ar : results) {
                        System.debug('37 PickupUpdates ar:'+ar);
                        Id lhId = (Id) ar.get('Linehaul_Tracking__c');
                        Decimal toatVal = (Decimal) ar.get('totalValue');
                        System.debug('40 PickupUpdates lhId:'+lhId);
                        System.debug('41 PickupUpdates toatVal:'+toatVal);
                        Linehaul_Tracking__c lhRec = lhMap.get(lhId);
                        System.debug('43 PickupUpdates lhRec:'+lhRec);
                        lhRec.Total_Shipment_Value__c = toatVal; 
                        toatValReturn+= toatVal;
                        System.debug('45 PickupUpdates pkRec:'+lhRec);
                        lhListToUpdate.add(lhRec);                       
                    }                    
                    if (!lhListToUpdate.isEmpty()) {
                        update lhListToUpdate;
                    }
                }
            } 
        }
        System.debug('46 toatValReturn:'+toatValReturn);
      return toatValReturn;  
    }    
