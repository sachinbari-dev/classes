    public static Decimal getTotalvalueFromShip(List<Shipment__c> shipList)
    {
        System.debug('51 PickupUpdates pkIdList:'+shipList);
        Decimal toatValReturn=0; 
        if(shipList.size()>0)
        {
			for(Shipment__c ship:shipList)     
            {
               toatValReturn+=ship.Shipment_Value__c; 
            }
        }
       return toatValReturn; 
    }
    
